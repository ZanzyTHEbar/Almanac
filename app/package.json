{
    "name": "almanac",
    "version": "0.1.0",
    "private": true,
    "description": "An open-source, cross-platform, and easy-to-use gardening calendar app built with SolidJS and Tauri.",
    "author": "DaOfficialWizard",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "tsc && vite build",
        "start": "vite start",
        "serve": "vite preview",
        "supabase:start": "supabase start",
        "supabase:stop": "supabase stop",
        "supabase:dev": "supabase functions serve --env-file supabase/.env",
        "supabase:deploy": "supabase secrets set --env-file ./supabase/.env && supabase functions deploy chat --import-map supabase/functions/import_map_deploy.json",
        "tauri": "tauri",
        "tauri:dev": "tauri dev",
        "tauri:dev:release": "tauri dev --release",
        "tauri:build": "tauri build",
        "tauri:build:dev": "tauri build --debug",
        "tauri:update": "cd src-tauri && cargo update && cargo upgrade",
        "tauri:clean": "cd src-tauri && cargo clean",
        "specta:post-install": "pnpm run specta:gen && pnpm run specta:build",
        "specta:build": "pnpm --stream -r specta:build",
        "specta:gen": "pnpm --stream -r specta:gen",
        "doc-gen": "jsdoc -c jsdoc.conf.json",
        "lint": "eslint --ext .js,.ts,.jsx,.tsx src",
        "format": "pnpm run lint --fix & pnpm prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
        "update-deps": "pnpm up -Li",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@babel/core": "^7.24.5",
        "@babel/preset-env": "^7.24.5",
        "@emotion/serialize": "^1.1.4",
        "@kobalte/tailwindcss": "^0.9.0",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@tauri-apps/api": "^1.5.5",
        "@tauri-apps/cli": "^1.5.14",
        "@types/lodash.merge": "^4.6.7",
        "@types/luxon": "^3.4.2",
        "@types/node": "^20.12.12",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.9.0",
        "@typescript-eslint/parser": "^7.9.0",
        "autoprefixer": "^10.4.19",
        "babel-loader": "^9.1.3",
        "babel-plugin-macros": "^3.1.0",
        "better-docs": "^2.7.3",
        "daisyui": "^4.11.1",
        "eslint": "^9.2.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-autofix": "^2.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-solid": "^0.14.0",
        "https-localhost": "^4.7.1",
        "jsdoc": "^4.0.3",
        "postcss": "^8.4.38",
        "postcss-import": "^16.1.0",
        "prettier": "^3.2.5",
        "supabase": "1.167.4",
        "tailwindcss": "^3.4.3",
        "tailwindcss-animate": "^1.0.7",
        "tailwindcss-brand-colors": "^0.3.6",
        "tailwindcss-debug-screens": "^2.2.1",
        "typescript": "^5.4.5",
        "vite": "^5.2.11",
        "vite-plugin-solid": "^2.10.2"
    },
    "dependencies": {
        "@kobalte/core": "^0.13.1",
        "@langchain/community": "^0.0.57",
        "@langchain/core": "0.1.63",
        "@langchain/langgraph": "^0.0.12",
        "@langchain/openai": "^0.0.29",
        "@microsoft/fetch-event-source": "^2.0.1",
        "@solid-primitives/i18n": "^2.1.1",
        "@solid-primitives/map": "^0.4.11",
        "@solid-primitives/memo": "^1.3.8",
        "@solid-primitives/reducer": "^0.0.101",
        "@solid-primitives/scheduled": "^1.4.3",
        "@solidjs/meta": "^0.29.3",
        "@solidjs/router": "^0.13.3",
        "@supabase/supabase-js": "^2.43.2",
        "@tailwindcss/typography": "^0.5.13",
        "@thisbeyond/solid-dnd": "^0.7.5",
        "babel-preset-solid": "^1.8.17",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.11",
        "dotenv": "^16.4.5",
        "ioredis": "^5.4.1",
        "langchain": "^0.1.33",
        "lodash.merge": "^4.6.2",
        "luxon": "^3.4.4",
        "redis": "^4.6.13",
        "solid-form-handler": "^1.2.3",
        "solid-headless": "^0.13.1",
        "solid-i18n": "^1.1.0",
        "solid-icons": "^1.1.0",
        "solid-js": "^1.8.17",
        "solid-spinner": "^0.2.0",
        "solid-transition-group": "^0.2.3",
        "solidjs-icons": "^1.0.12",
        "solidjs-use": "^2.3.0",
        "tailwind-merge": "^2.3.0",
        "tauri-plugin-log-api": "github:tauri-apps/tauri-plugin-log",
        "tauri-plugin-splashscreen": "workspace:*",
        "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store",
        "tauri-plugin-upload-api": "github:tauri-apps/tauri-plugin-upload",
        "terracotta": "^1.0.5",
        "theme-change": "^2.5.0",
        "ts-deepmerge": "^7.0.0",
        "uuid": "^9.0.1",
        "yup": "^1.4.0",
        "zod": "^3.23.8"
    }
}
